import java.util.Scanner;
public class factorial{  
    public static void main(String args[]){  
    int i,fact=1,n;
    Scanner in=new Scanner(System.in);
    System.out.print("enter a number=");
    n=in.nextInt();
    for(i=1;i<=n;i++){    
        fact=fact*i;    
    }    
    System.out.println("Factorial of "+n+" is: "+fact);    
 } 
}  
